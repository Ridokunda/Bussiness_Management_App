<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/regCUstomer.css' />
  </head>
  <body>
    <%- include('./partials/head.ejs') %>
    <div class="container">
        <form class="reg_customer" action="" method="post">
            <h1>Log in</h1>
            <fieldset>
                <label for="cus_email">Email</label>
                <input type="email" placeholder="abc@mail.com" name="cus_email" id="cus_email">
            </fieldset>
           
            <fieldset>
                <label for="password">Password</label>
                <input type="password" id="password" name="password">
            </fieldset>
            <button type="submit" id="btn_reg_customer">Log in</button>
        </form>
    </div>
    <%- include('./partials/footer') %>

  </body>
</html>